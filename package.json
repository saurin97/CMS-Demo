		"@types/classnames": "^2.2.7",
		"@types/node": "11.13.7",
		"@types/react": "16.8.14",
		"@types/react-dom": "16.8.4",
		"@types/uuid": "^8.3.0",
		"aws-sdk": "^2.624.0",
		"axios": "^0.18.0",
		"babel-preset-stage-2": "^6.24.1",
		"bootstrap": "3.3.7",
		"bootstrap-v4-rtl": "^4.1.1-0",
		"can-use-dom": "^0.1.0",
		"classnames": "^2.2.6",
		"cross-env": "^5.2.0",
		"crypto-js": "^3.1.9-1",
		"datatables.net": "^1.10.19",
		"date-fns": "^2.4.1",
		"font-awesome": "^4.7.0",
		"formik": "^1.5.4",
		"history": "^4.7.2",
		"install": "^0.10.1",
		"lodash": "^4.17.15",
		"moment": "^2.22.2",
		"node_env": "^0.0.3",
		"nprogress": "^0.2.0",
		"path": "^0.12.7",
		"query-string": "^6.5.0",
		"react": "^16.8.6",
		"react-bootstrap": "^0.32.4",
		"react-custom-scrollbars": "^4.2.1",
		"react-datepicker": "^2.8.0",
		"react-dom": "^16.8.6",
		"react-redux": "^6.0.1",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1",
		"react-scripts": "3.0.0",
		"react-table": "^6.8.6",
		"react-toastify": "^4.4.3",
		"recompose": "^0.30.0",
		"redux": "^4.0.1",
		"redux-logger": "^3.0.6",
		"redux-saga": "^0.16.2",
		"redux-thunk": "^2.3.0",
		"save": "^2.3.2",
		"typeface-roboto": "^0.0.54",
		"typescript": "3.4.5",
		"url-search-params": "^1.1.0",
		"uuid": "^8.3.1",
		"yup": "^0.27.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build --max_old_space_size=900",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint src/**/*.{ts,tsx}"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/crypto-js": "^3.1.43",
		"@types/lodash": "^4.14.125",
		"@types/query-string": "^6.3.0",
		"@types/react-bootstrap": "^0.32.17",
		"@types/react-custom-scrollbars": "^4.0.5",
		"@types/react-datepicker": "^2.3.0",
		"@types/react-redux": "^7.0.8",
		"@types/react-router-dom": "^4.3.2",
		"@types/redux": "^3.6.0",
		"@types/redux-thunk": "^2.1.0",
		"@types/yup": "^0.26.12",
		"eslint-plugin-react": "^7.13.0",
		"husky": "^2.2.0",
		"lint-staged": "^8.1.6",
		"mrm": "^1.2.1",
		"node-sass": "^4.9.3",
		"tslint-config-airbnb": "^5.11.1",
		"tslint-config-standard": "^8.0.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"eslint --fix",
			"git add"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"plugin:react/recommended"
		],
		"plugins": [
			"react"
		],
		"rules": {
			"react/prop-types": [
				0
			],
			"react/no-redundant-should-component-update": [
				2
			],
			"react/no-typos": [
				2
			],
			"react/no-this-in-sfc": [
				2
			],
			"react/no-unescaped-entities": [
				2
			],
			"react/no-unknown-property": [
				2
			],
			"react/no-unsafe": [
				2
			],
			"react/no-unused-state": [
				2
			],
			"react/no-will-update-set-state": [
				2
			],
			"react/prefer-stateless-function": [
				1
			],
			"react/self-closing-comp": [
				1
			],
			"react/jsx-no-bind": [
				1
			],
			"react/jsx-fragments": [
				1,
				"syntax"
			],
			"react/jsx-pascal-case": [
				2
			],
			"react/sort-comp": [
				2,
				{
					"order": [
						"instance-variables",
						"static-methods",
						"lifecycle",
						"render",
						"everything-else"
					],
					"groups": {
						"instance-variables": [
							"state"
						]
					}
				}
			]
		}
	}
}
